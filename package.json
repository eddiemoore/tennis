{
  "name": "tennis",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "npm run lint && lint-staged",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eddiemoore/tennis"
  },
  "bugs": {
    "url": "https://github.com/eddiemoore/tennis/issues"
  },
  "homepage": "https://github.com/eddiemoore/tennis",
  "devDependencies": {
    "@types/jest": "23.3.1",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "ts-jest": "23.1.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.0.3"
  },
  "lint-staged": {
    "**/*.{ts,js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/lib/**"
    ]
  }
}
